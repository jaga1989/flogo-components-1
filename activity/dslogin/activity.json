{
  "name": "dslogin",
  "type": "flogo:activity",
  "ref": "github.com/jvanderl/flogo-components/activity/dslogin",
  "version": "0.0.1",
  "title": "3DS Login",
  "description": "Login to the Dassault 3DEXPERIENCE Platform",
  "homepage": "https://github.com/jvanderl/flogo-components/tree/master/activity/dslogin",
  "input":[
    {
      "name": "3DPassportURL",
      "type": "string",
      "required": true
    },
    {
      "name": "3DServiceURL",
      "type": "string",
      "required": true
    },
    {
      "name": "userName",
      "type": "string",
      "required": true
    },
    {
      "name": "serviceName",
      "type": "string",
      "required": true
    },
    {
      "name": "serviceSecret",
      "type": "string",
      "required": true
    },
    {
      "name": "skipSsl",
      "type": "boolean",
      "value": false
    }
  ],
  "output": [
    {
      "name": "serviceAccessToken",
      "type": "string"
    },
    {
      "name": "serviceRedirectURL",
      "type": "string"
    },
    {
      "name": "status",
      "type": "integer"
    }
  ]
}
